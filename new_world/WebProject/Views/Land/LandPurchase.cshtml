@using System.Security.Claims;
@using WebProject.Models.ViewModels;

@{
    Layout = "";
}
<!doctype html>
<html lang="en">

  <head>
    <title>WaxVing - Land purchase</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="~/Jaroslav/https://fonts.googleapis.com/css?family=Rubik:300,400,700|Oswald:400,700" rel="stylesheet">
    <link rel="icon" href="~/Jaroslav/foto/logo.svg" type="image/x-icon">
    <link rel="stylesheet" href="~/Jaroslav/fonts/icomoon/style.css">

    <link rel="stylesheet" href="~/Jaroslav/css/bootstrap.min.css">
