@model List<Land>

<div class="container">
    <form method="post" asp-action="MakeMoney">
        <input name="amount" />
        <input type="submit" />
    </form>

    <table class="table table-bordered">
        <thead class="thead-dark">
            <tr>
                <th>#</th>
                <th>User-id</th>
                <th>Area-id</th>
            </tr>
        </thead>
        <tbody>
        @for (int i = 0; i < Model.Count && i < 50; i++)
        {
            <tr>
                <td>@Model[i].ID</td>
                <td>@Model[i].UserId</td>
                <td>@Model[i].AreaId</td>
            </tr>
        }
        </tbody>
    </table>
</div>